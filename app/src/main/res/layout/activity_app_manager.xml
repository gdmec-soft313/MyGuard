<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cn.edu.gdmec.chaos07150844.myguard.m4appmanager.AppManagerActivity">
    <include layout="@layout/titlebar"/>
    <RelativeLayout
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_phonememory_appmanager"
            android:layout_alignParentLeft="true"
            style="@style/textview12sp" />
        <TextView
            android:id="@+id/tv_sdmemory_appmanager"
            android:layout_alignParentRight="true"
            style="@style/textview12sp" />
    </RelativeLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ListView
            android:id="@+id/lv_appmanager"
            android:listSelector="@color/transparent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></ListView>
        <TextView
            android:id="@+id/tv_appnumber"
            android:padding="5dp"
            android:textColor="@color/black"
            android:background="@color/graye5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </FrameLayout>

</LinearLayout>
